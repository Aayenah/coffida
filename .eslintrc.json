{
  "env": {
      "browser": true,
      "es6": true,
      "node": true
  },
  "extends": [
      "plugin:prettier/recommended",
      "plugin:react/recommended",
      "airbnb"
  ],
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
      "ecmaFeatures": {
          "jsx": true
      },
      "ecmaVersion": 2018,
      "sourceType": "module"
  },
  "plugins": [
      "react"
  ],
  "rules": {
    "react/jsx-filename-extension": [1, {"extensions": [".js", ".jsx"]}],
    "prettier/prettier": ["warn", {"bracketSpacing": true, "endOfLine": "auto", "jsxBracketSameLine": false}],
    "arrow-body-style": "off",
    "no-use-before-define": ["error", { "functions": true, "classes": true, "variables": false }],
    "no-unused-vars": "warn",
    "object-curly-newline": "off"
  }
}